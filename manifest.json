{
    "name": "Ababil Homepage",
    "manifest_version": 3,
    "version": "1.1.1",
    "title": "Browser Homepage",
    "description": "A minimalist browser home or newtab for Professionals. It allows to choose your favorite search Engine.",    
    "permissions":[
        "storage"
    ],
    "action": {
        "default_icon": "/image/logo.png"
    },
    "icons":{
        "64": "/image/logo.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "script.js"
            ]
        }
    ]
}
